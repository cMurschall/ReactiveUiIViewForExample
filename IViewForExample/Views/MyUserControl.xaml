<reactiveUi:ReactiveUserControl x:TypeArguments="viewModels:MyUserControlViewModel" x:Class="IViewForExample.Views.MyUserControl"
                                xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                                xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                                xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                                xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                                xmlns:local="clr-namespace:IViewForExample.Views"
                                xmlns:reactiveUi="http://reactiveui.net"
                                xmlns:viewModels="clr-namespace:IViewForExample.ViewModels"
                                mc:Ignorable="d"
                                d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
        <Label Content="Hello from MyUserControl"/>

        <TextBox x:Name="AppNameTextBox"/>
    </StackPanel>
</reactiveUi:ReactiveUserControl>
